<div {{microdata}}>
    {{#idAsUrl}}
        <h1 itemprop="name">{{name}}</h1>
        <link href="{{path}}./" {{id.microdata}}/>
        <link href="{{path}}./?f=html" itemprop="hasMap"/>
        {{#requestUrl}}
        <link href="{{.}}" itemprop="sameAs"/>
        {{/requestUrl}}
    {{/idAsUrl}}
    {{^idAsUrl}}
        <h4><a href="{{path}}{{id.value}}{{^noUrlClosingSlash}}/{{/noUrlClosingSlash}}" {{id.microdata}}><span itemprop="name">{{name}}</span></a></h4>
    {{/idAsUrl}}
        {{#children}}
            {{> property}}
        {{/children}}
        {{#geo}}
            {{> property}}
        {{/geo}}
        {{#links}}
            <dl class="dl-horizontal dl-horizontal-no-space">
                <dt>{{name}}</dt>
            <dd>
                {{#children}}
                    <a href="{{value.value}}" target="_blank">{{value.name}}</a>{{^last}}, {{/last}}
                {{/children}}
            </dd>
            </dl>
        {{/links}}
</div>