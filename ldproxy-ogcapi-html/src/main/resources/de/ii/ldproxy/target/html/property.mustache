{{#classic}}
    {{#values}}
        <div class="row my-1">
            <div class="col-md-6 {{#isLevel2}}pl-4x {{/isLevel2}}{{#isLevel3}}pl-5 {{/isLevel3}}{{#isObjectSeparator}}object-separator{{/isObjectSeparator}}{{^isObjectSeparator}}font-weight-bold{{/isObjectSeparator}} text-truncate" title="{{name}}">{{name}}</div>
            <div class="col-md-6">{{#isUrl}}<a target="_blank" href="{{value}}">{{value}}</a>{{/isUrl}}{{^isUrl}}{{#isImg}}<img src="{{value}}" class="img-fluid"/>{{/isImg}}{{^isImg}}{{#isHtml}}{{{value}}}{{/isHtml}}{{^isHtml}}{{value}}{{/isHtml}}{{/isImg}}{{/isUrl}}</div>
        </div>
    {{/values}}
    {{#objectValues}}
        <div class="row my-1">
            <div class="col-md-12 {{#isLevel2}}pl-4x {{/isLevel2}}{{#isLevel3}}pl-5 {{/isLevel3}}{{#isObjectSeparator}}object-separator{{/isObjectSeparator}}{{^isObjectSeparator}}font-weight-bold{{/isObjectSeparator}} text-truncate" title="{{name}}">{{name}}</div>
        </div>
        {{#properties}}
            {{> property}}
        {{/properties}}
    {{/objectValues}}
{{/classic}}
{{#complexObjects}}
    {{#values}}
<div class="row my-1">
<div class="col-md-4 {{#isLevel2}}pl-4x {{/isLevel2}}{{#isLevel3}}pl-5 {{/isLevel3}}{{#isObjectSeparator}}object-separator{{/isObjectSeparator}}{{^isObjectSeparator}}font-weight-bold{{/isObjectSeparator}} text-truncate" title="{{name}}">{{name}}</div>
<div class="col-md-8">{{#isUrl}}<a target="_blank" href="{{value}}">{{value}}</a>{{/isUrl}}{{^isUrl}}{{#isImg}}<img src="{{value}}" class="img-fluid"/>{{/isImg}}{{^isImg}}{{#isHtml}}{{{value}}}{{/isHtml}}{{^isHtml}}{{value}}{{/isHtml}}{{/isImg}}{{/isUrl}}</div>
</div>
    {{/values}}
    {{#objectValues}}
        <div class="row my-1">
            <div class="col-md-12 {{#isLevel2}}pl-4x {{/isLevel2}}{{#isLevel3}}pl-5 {{/isLevel3}}{{#isObjectSeparator}}object-separator{{/isObjectSeparator}}{{^isObjectSeparator}}font-weight-bold{{/isObjectSeparator}} text-truncate" title="{{name}}">{{name}}</div>
        </div>
        {{#properties}}
            {{> property}}
        {{/properties}}
    {{/objectValues}}
{{/complexObjects}}


