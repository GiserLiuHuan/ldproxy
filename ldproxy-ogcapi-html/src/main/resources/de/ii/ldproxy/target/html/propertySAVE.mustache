{{#index}}
    {{#value.children}}
        {{#first}}
            <span>
        {{/first}}
        {{> property}}
        {{#last}}
            </span>
        {{/last}}
        <div class="row my-1">
            {{#classic}}
                <div class="col-md-6 font-weight-bold text-truncate" title="{{value.name}}">{{value.name}}</div>
                <div class="col-md-6">{{#value.isUrl}}<a target="_blank" href="{{value.value}}">{{value.value}}</a>{{/value.isUrl}}{{^value.isUrl}}{{#value.isImg}}<img src="{{value.value}}" class="img-fluid"/>{{/value.isImg}}{{^value.isImg}}{{#value.isHtml}}{{{value.value}}}{{/value.isHtml}}{{^value.isHtml}}{{value.value}}{{/value.isHtml}}{{/value.isImg}}{{/value.isUrl}}</div>
            {{/classic}}
            {{#complexObjects}}
                <div class="col-md-4 {{#value.isLevel2}}pl-4x {{/value.isLevel2}}{{#value.isLevel3}}pl-5 {{/value.isLevel3}}{{#value.isObjectSeparator}}object-separator{{/value.isObjectSeparator}}{{^value.isObjectSeparator}}font-weight-bold{{/value.isObjectSeparator}} text-truncate" title="{{value.name}}">{{value.name}}</div>
                <div class="col-md-8">{{#value.isUrl}}<a target="_blank" href="{{value.value}}">{{value.value}}</a>{{/value.isUrl}}{{^value.isUrl}}{{#value.isImg}}<img src="{{value.value}}" class="img-fluid"/>{{/value.isImg}}{{^value.isImg}}{{#value.isHtml}}{{{value.value}}}{{/value.isHtml}}{{^value.isHtml}}{{value.value}}{{/value.isHtml}}{{/value.isImg}}{{/value.isUrl}}</div>
            {{/complexObjects}}
        </div>
    {{/value.children}}
    {{^value.children}}
        {{#values}}
            <div class="row my-1">
                {{#classic}}
                    <div class="col-md-6 font-weight-bold text-truncate" title="{{name}}">{{name}}</div>
                    <div class="col-md-6">{{#isUrl}}<a target="_blank" href="{{value}}">{{value}}</a>{{/isUrl}}{{^isUrl}}{{#isImg}}<img src="{{value}}" class="img-fluid"/>{{/isImg}}{{^isImg}}{{#isHtml}}{{{value}}}{{/isHtml}}{{^isHtml}}{{value}}{{/isHtml}}{{/isImg}}{{/isUrl}}</div>
                {{/classic}}
                {{#complexObjects}}
                    <div class="col-md-4 {{#isLevel2}}pl-4x {{/isLevel2}}{{#isLevel3}}pl-5 {{/isLevel3}}{{#isObjectSeparator}}object-separator{{/isObjectSeparator}}{{^isObjectSeparator}}font-weight-bold{{/isObjectSeparator}} text-truncate" title="{{name}}">{{name}}</div>
                    <div class="col-md-8">{{#isUrl}}<a target="_blank" href="{{value}}">{{value}}</a>{{/isUrl}}{{^isUrl}}{{#isImg}}<img src="{{value}}" class="img-fluid"/>{{/isImg}}{{^isImg}}{{#isHtml}}{{{value}}}{{/isHtml}}{{^isHtml}}{{value}}{{/isHtml}}{{/isImg}}{{/isUrl}}</div>
                {{/complexObjects}}
            </div>
        {{/values}}
        {{#children}}
            {{#children}}
                {{> property}}
            {{/children}}
        {{/children}}
    {{/value.children}}
{{/index}}